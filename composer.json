{
    "name": "alessandro-nuunes/filament-tabbed-dashboard",
    "description": "A Filament plugin for FilamentTabbedDashboard.",
    "keywords": [
        "filament",
        "filament-plugin",
        "laravel"
    ],
    "homepage": "https://github.com/alessandro-nuunes/filament-tabbed-dashboard",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/alessandro-nuunes/filament-tabbed-dashboard/issues",
        "source": "https://github.com/alessandro-nuunes/filament-tabbed-dashboard"
    },
    "authors": [
        {
            "name": "AlessandroNuunes",
            "email": "alessandronuunes@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "spatie/laravel-package-tools": "^1.16",
        "filament/filament": "^4.0|^5.0",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AlessandroNuunes\\FilamentTabbedDashboard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlessandroNuunes\\FilamentTabbedDashboard\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlessandroNuunes\\FilamentTabbedDashboard\\FilamentTabbedDashboardServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
